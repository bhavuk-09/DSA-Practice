#include <bits/stdc++.h>
using namespace std;
int main() {
    int n,i;
    int min=INT_MAX;
    int max= INT_MIN;
    cin>>n;
    int arr[n];
    for(i=0;i<n;i++){
        cin>>arr[i];
        if(arr[i]<min){
            min=arr[i];
        }
        if(arr[i]>max){
            max= arr[i];
        }
    }
    cout<<"Largest Element:"<<max<<endl;
    cout<<"Smallest Element:"<<min<<endl;
}
