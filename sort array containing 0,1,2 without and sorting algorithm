//FUNCTION

void sort012(int a[], int n)
{   int a1=0,b=0,c=0;
    for(int i=0;i<n;i++){
        if(a[i]==0){
            a1++;
        }
        if(a[i]==1){
            b++;
        }
        if(a[i]==2)
            c++;
    }
    for(int i=0;i<a1;i++){
        a[i]=0;
    }
    for(int i=a1;i<=(b+a1);i++){
        a[i]=1;
    }
    for(int i=b+a1;i<(c+a1+b);i++){
        a[i]=2;
    }
    
    
}
